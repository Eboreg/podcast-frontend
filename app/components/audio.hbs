<audio
  {{on-audio-insert this.setAudioElement}}
  {{on "canplay" this.refreshBufferEnd}}
  {{on "canplaythrough" this.refreshBufferEnd}}
  {{on "durationchange" this.onDurationChange}}
  {{on "ended" this.onEnded}}
  {{on "loadeddata" this.refreshBufferEnd}}
  {{on "pause" this.onPause}}
  {{on "play" this.onPlay}}
  {{on "playing" this.onPlay}}
  {{on "progress" this.refreshBufferEnd}}
  {{on "ratechange" this.onRateChange}}
  {{on "seeked" this.onSeeked}}
  {{on "seeking" this.onSeeking}}
  {{on "timeupdate" this.onTimeUpdate}}
  {{on "volumechange" this.onVolumeChange}}
  ...attributes
></audio>
